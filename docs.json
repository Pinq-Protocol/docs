{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Pinq Protocol Docs",
  "colors": {
    "primary": "#FF6F00",
    "light": "#F9F9F9",
    "dark": "#212121"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
